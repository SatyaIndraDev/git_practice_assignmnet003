function prime (number){
let fact=0
for(let i=1;i<=number;i++)
  {
    if(number%i==0){
      fact++
    }
  }
if(fact==2)
{
  return true
}
else{
  return false
}
}
for(let j=2;j<=10;j++)
  {
  let answer=prime(j)
    if(answer==true)
    {
      console.log(j,"is a prime")
    }
    else{
       console.log(j,"is a not prime")
    }
  }